.Localsearch{
	background: #f2f2f2;
	position: fixed;
	z-index: 9999;
	top: 44px;
	left: 0px;
	padding: 2px 10px;
	max-width: 640px;
}
.Localsearch input[type=search] {
    margin: 7px 0px;
    margin-right: 10px;
    font-size: 14px;
    width: 100%;
    max-width: 640px;
    text-align: left;
    padding: 6px 0px 6px 29px;  
    border-radius:29px ;   
    background: #fff;
    text-indent: 5px;
}
.Localsearch  span{
	position: absolute;
    top: 14px;
    left: 10px;
}
body,.mui-content{
	background: #f2f2f2;
}
