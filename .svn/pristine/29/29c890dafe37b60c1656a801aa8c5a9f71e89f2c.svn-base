package com.lanzhou.entity;

public class Problems {
	private int id;
	private String title;//常见问题
	private String content;//内容
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Problems(int id, String title, String content) {
		super();
		this.id = id;
		this.title = title;
		this.content = content;
	}
	public Problems(String title, String content) {
		super();
		this.title = title;
		this.content = content;
	}
	public Problems() {
		super();
	}
	

}
