		<script type="text/javascript">
			mui.init();
			//顶部导航
			var ListNavService = document.getElementById("ListNavService")
			var odiv = ListNavService.getElementsByTagName("li");
			var newlist1 = document.getElementsByClassName("myOrderList");
 
			odiv[0].className = 'ontopOk';
			newlist1[0].style.display = "block";
			for(var a = 0; a < odiv.length; a++) {
				odiv[a].index = a;
				odiv[a].onclick = function() {
					for(var b = 0; b < newlist1.length; b++) {
						odiv[b].className = '';
						newlist1[b].style.display = 'none';
					}
					this.className = 'ontopOk';
					newlist1[this.index].style.display = "block";

				}
			};
			//申请退款弹窗tuikuanGoods_box_h
			var tuikuanGoods_ =$(".tuikuanBtns");
			for (var i = 0; i < tuikuanGoods_.length; i++) {
			    (function(e){  //自执行函数实时把i传入e
			        tuikuanGoods_[e].onclick=function(){  //  通过传入的e获取到哪个按钮发生了点击事件
//			            alert(e) //弹出e（此处e为i实时对应值）
                       $(".tuikuanGoods_box_h").css("transform","translateY(0px)"); 
			        }
			    })(i)    
			};   //采用闭包和自执行函数获取i
			//关闭申请退款弹窗
			$(".deliverGoods_box_coles").click(function(){
				$(".tuikuanGoods_box_h").css("transform","translateY(-1000px)"); 
			});
			$(".coles_2").click(function(){
				$(".tuikuanGoods_box_h").css("transform","translateY(-1000px)"); 
			});
			//删除订单弹窗   
			var delBtns_ =$(".delBtns");
			for (var i = 0; i < delBtns_.length; i++) {
			    (function(e){  //自执行函数实时把i传入e
			        delBtns_[e].onclick=function(){  //  通过传入的e获取到哪个按钮发生了点击事件
//			            alert(e) //弹出e（此处e为i实时对应值）
                       $(".delGoods_box_h").css("transform","translateY(0px)"); 
			        }
			    })(i)    
			};   //采用闭包和自执行函数获取i
			//关闭删除订单弹窗
			$(".deliverGoods_box_coles").click(function(){
				$(".delGoods_box_h").css("transform","translateY(-1000px)"); 
			});
			$(".coles_2").click(function(){
				$(".delGoods_box_h").css("transform","translateY(-1000px)");
			});
			//再来一单
			$(".zailaiorderBtns").click(function(){
				setTimeout(function(){
					mui.openWindow({
						url: 'details.html',
						id: 'details',
						showType: 'zoom-fade-out',
						showTime: 200
					});
				},200);
			});
			//关闭窗口
			function CloseWin() //这个不会提示是否关闭浏览器    
			{    
				var userAgent = navigator.userAgent;
				if (userAgent.indexOf("Firefox") != -1 || userAgent.indexOf("Chrome") !=-1) {
				   window.location.href="about:blank";
				}else if(userAgent.indexOf('Android') > -1 || userAgent.indexOf('Linux') > -1){
				   window.opener=null;window.open('about:blank','_self','').close();
				}else {
				   window.opener = null;
				   window.open("about:blank", "_self");
				   window.close();
				}
			}   
		</script>

