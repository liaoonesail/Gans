		<script>
			mui.init();
			/**
			 * 根据label标签（1、2）{学生、家长}获取所有中奖人的信息
			 * @param response
			 * @param label
			 * @return
			 * @throws IOException 
			 */
			 function PrizephonegetList(label){
				    var url = "userSp/getList.do";
					var data = {
							label:label
				    };
					async = false;
					var result = ajaxSubmit(url, data, async);
					//console.log(result)
					return result;
			}
			$(document).ready(function(){
				//学生礼包
				
				//通过PrizephonegetList(label)方法得到数据,label的参数为1(学生),获取所有学生的数据集合
				var getCheckedArea_true =PrizephonegetList(1);
				//转换JSON对象
				getCheckedArea_true = JSON.parse(getCheckedArea_true);
				//打印
				//console.log(getCheckedArea_true)
				//清空节点内容
				$("#giftLiContent").empty();
				//定义变量
				var giftLiContent_ ="";
				//循环得到数据的长度
				for(var i = 0;i<getCheckedArea_true.length;i++){
					giftLiContent_+="<li>"
						+"<div class='giftModelScorll clearfix'>"
							+"<p class='giftModelTop'><span class='giftModelTopl'>"+getCheckedArea_true[i].phone.substr(0, 3) + '****' + getCheckedArea_true[i].phone.substr(7)+"</span><span class='giftModelTopr'>"+getCheckedArea_true[i].time+"</span></p>"
							+"<p class='giftModelContent'>"+getCheckedArea_true[i].prize+"</p>"
						+"</div>"
					+"</li>";
					
				}
				//添加到所对应节点，并显示所有内容
				$("#giftLiContent").append(giftLiContent_);
				
				/*id:stScorllOne下面的ul li 个数大于 4 条才能滚动*/
				if($("#stScorllOne ul").children("li").length > 4){
		        	$("#stScorllOne").textSlider({line:1,speed:500,timer:3000});
				}
				

				//家长礼包
				var getCheckedArea_true2 =PrizephonegetList(2);
				getCheckedArea_true2 = JSON.parse(getCheckedArea_true2);
				//console.log(getCheckedArea_true2)
				$("#patrLiContent").empty();
				var giftLiContent_2 ="";
				for(var i = 0;i<getCheckedArea_true2.length;i++){
					giftLiContent_2+="<li>"
						+"<div class='giftModelScorll clearfix'>"
							+"<p class='giftModelTop'><span class='giftModelTopl'>"+getCheckedArea_true2[i].phone.substr(0, 3) + '****' + getCheckedArea_true2[i].phone.substr(7)+"</span><span class='giftModelTopr'>"+getCheckedArea_true2[i].time+"</span></p>"
							+"<p class='giftModelContent'>"+getCheckedArea_true2[i].prize+"</p>"
						+"</div>"
					+"</li>";
					
				}
				$("#patrLiContent").append(giftLiContent_2);

				/*id:stScorllTwo下面的ul li 个数大于 4 条才能滚动*/
				if($("#stScorllTwo ul").children("li").length > 4){
		        	$("#stScorllTwo").textSlider({line:1,speed:500,timer:3000});
				}
			});

