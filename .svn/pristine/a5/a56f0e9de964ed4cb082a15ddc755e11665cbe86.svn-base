	<script>
		mui.init();
		$("#searchCode2").click(function(){
			var searchName = $("#searchName").val();
			if(searchName == ""){
				alert("请输入搜索商品名称")
			}else{
				setTimeout(function(){
					mui.openWindow({
						url: 'stage/discountgo/search_proud.html?searchName='+searchName,
						id: 'search_proud',
						showType: 'zoom-fade-out',
						showTime: 200
					});
				},200); 
			 $("#searchName").val("");
			}
			
		});
		//判断是IOS还是android
	    var  datatype="";		
		if (/(iPhone|iPad|iPod|iOS)/i.test(navigator.userAgent)) {  //判断iPhone|iPad|iPod|iOS
		    datatype ="ios";
		} else if (/(Android)/i.test(navigator.userAgent)) {   //判断Android
		    datatype ="android";
		} else {  //pc
			datatype ="";
		};

		$.ajax({
	        type: "POST",
	        url: "/Gans/showUserSystem.do",
	        data:{zd:datatype},
	        cache:false,  
			dataType:'html',
			async:false,
	        success: function(data){
	        	console.log(data)
	        }
	    });
	</script>

