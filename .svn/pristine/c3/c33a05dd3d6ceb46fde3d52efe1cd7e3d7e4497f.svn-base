.discoNav{
	margin: 14px 0px 0px 0px;
	height: 80px;
}
.discoNavLi{
	width: 60px;
	height: 60px;
	border-radius: 60px;
	margin: 0 auto;
	overflow: hidden;
}
.discoNavLiImg1{
	display: block;
    width: 34px;
    height: 30px;
    margin: 15px auto;
}
.discoNavLiImg2{
	display: block;
    width: 33px;
    height: 25px;
    margin: 15px auto;
}
.discoNavLiImg3{
	display: block;
    width: 26px;
    height: 33px;
    margin: 13px auto;
}
.discoNavLiImg4{
	display: block;
    width: 35px;
    height: 27px;
    margin: 15px auto;
}
.discoNav .mui-col-xs-3:nth-of-type(1) .discoNavLi{
	background: #ff9501;
}
.discoNav .mui-col-xs-3:nth-of-type(2) .discoNavLi{
	background: #f26665;
}
.discoNav .mui-col-xs-3:nth-of-type(3) .discoNavLi{
	background: #1bce8d;
}
.discoNav .mui-col-xs-3:nth-of-type(4) .discoNavLi{
	background: #ff784b;
}
.discoNav .mui-col-xs-3 p{
	text-align: center;
	margin-bottom: 0px;
	height: 20px;
	line-height: 20px;
	color: #000;
	font-size: 14px;
}
.noticeBox12{
	border-bottom: 4px solid #e5e5e5;
}
.preferentialZonebox{
	width: 100%;
	padding: 10px 4px;
	position: relative;
	border-top: 7px solid #e5e5e5;
}
.preferentialZonebox h3{
	font-weight: normal;
	font-size: 15px;
	text-align: center;
	position: relative;
	padding-bottom: 6px;
}
.preferentialZonebox h3:before{
	content: "";
    width: 23%;
    height: 1px;
    background: #aaaaaa;
    left: 13%;
    top: 6px;
    position: absolute;
}
.preferentialZonebox h3:after{
	content: "";
    width: 23%;
    height: 1px;
    background: #aaaaaa;
    right: 13%;
    top: 6px;
    position: absolute;
}

/**/
.preferentialZonebox .mui-row .mui-col-xs-4{
	padding: 2px;
}
.preferentialZonebox .mui-row .mui-col-xs-4 img{
	display: block;
	width: 100%;
	height: 80px;
}
.preferentialZonebox .mui-row .mui-col-xs-4 p{
	font-size: 13px;
	text-align: center;
	margin-bottom: 0px;
	color: #000;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
