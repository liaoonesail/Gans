.advertisementBox{
	padding: 4px 0px;
	background: #e5e5e5;
}
.advertisementBoxList{
	background: #fff;
	padding: 2px 0px;
}
.advertisementBoxList1{
	float: left;
}
.advertisementBoxList1 img{
	display: block;
	width: 100%;
	height: 100%;
}
.advertisementBoxList1:nth-of-type(1){
	width: 35%;
    height: 151px;
}
.advertisementBoxList1:nth-of-type(2){
	width: 64%;
	float: right;
	height: 75px;
}
.advertisementBoxList1:nth-of-type(3){
	width: 31%;
	float: left;
	margin-left: 3px;
	height: 75px;
	margin-top: 1px;
}
.advertisementBoxList1:nth-of-type(4){
	width: 32%;
	float: right;
	height: 75px;
	margin-top: 1px;
}
/**/
.recommendBox{
	width: 100%;
	padding: 10px 4px;
}
.recommendBox h3{
	font-weight: normal;
	font-size: 15px;
	text-align: center;
	position: relative;
	padding-bottom: 6px;
}
.recommendBox h3:before{
	content: "";
    width: 23%;
    height: 1px;
    background: #aaaaaa;
    left: 13%;
    top: 6px;
    position: absolute;
}
.recommendBox h3:after{
	content: "";
    width: 23%;
    height: 1px;
    background: #aaaaaa;
    right: 13%;
    top: 6px;
    position: absolute;
}
.recommendBox .mui-row .mui-col-xs-6{
	border-top: 1px solid #E5E5E5;
	padding: 2px;
	height:260px;
	overflow: hidden;
}
.recommendBox .mui-row .mui-col-xs-6:nth-of-type(odd){
	border-right: 1px solid #E5E5E5;
}
.recommendBox .mui-row .mui-col-xs-6:nth-last-of-type(1){
	border-bottom: 1px solid #E5E5E5;
}
.recommendBox .mui-row .mui-col-xs-6:nth-last-of-type(2){
	border-bottom: 1px solid #E5E5E5;
}
.recommendBox .mui-row .mui-col-xs-6  img{
	display: block;
	width: 100%;
	height: 120px;
}
.recommendBox .mui-row .mui-col-xs-6 p{
	text-align: justify;
	color: #000;
	font-size: 14px;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 0px;
    text-align: left;
}
.recommendBox .mui-row .mui-col-xs-6 dd del{
	font-size: 12px;
	color: #898c8d;
	text-align: center;
	float: left;
}
.recommendBox .mui-row .mui-col-xs-6 dt{
	font-size: 12px;
	color: #000;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.recommendBox .mui-row .mui-col-xs-6 dd img{
	float: left;
    width: 45px;
    height: 20px;
    margin-left: 8px;
    margin-top: -6px;
}
.recommendBox .mui-row .mui-col-xs-6  .picdetails{
	font-size: 14px;
	color: #3d3d3d;
}
.recommendBox .mui-row .mui-col-xs-6  .picdetails span{
	color: #fe4878;
	font-size: 18px;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.recommendBox .mui-row .mui-col-xs-6  .picBttns{
	height: 32px;
	/*width: 70px;*/
	margin:4px auto;
	padding:0 15px;
	display: block;
	border: none;
	border-radius: 35px;
	border:2px solid #09b6f2;
	color: #09b6f2;
	font-size: 14px;
	text-align: center;
}
.recommendBox .mui-row .mui-col-xs-6  .picBttns:active{
	border:2px solid #164a99;
	color: #164a99;
}
