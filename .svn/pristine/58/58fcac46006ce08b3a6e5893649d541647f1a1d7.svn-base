body,.mui-content{
	background: #F2F2F2;
}
.topNva{
	height: 48px;
	position: fixed;
	top: 44px;
	z-index: 111111;
	background:#ffffff ;
	max-width: 640px;
}
.topNvaUlLi{
	width: 82%;
	margin-left: 60px;
	height:48px;
	position: relative;
}
.Shape1{
	position: absolute;
    width: 33px;
    height: 33px;
    left: 23px;
    top: 9px;
}
.topNvaUlLi .mui-segmented-control.mui-scroll-wrapper li {
    display: inline-block;
    padding: 0 16px;
    color: #000;
    font-size: 14px;
    text-align: center;
}
.topNvaUlLi .mui-segmented-control.mui-scroll-wrapper li span:nth-of-type(1){
	font-weight: bold;
	line-height: 19px;
}
.ontopOk{
	color: #ff0000 !important;
/* 	border-bottom: 2px solid  #02a1fc; */
}
.ontopOk p{
	color: #ff0000 !important;
}
