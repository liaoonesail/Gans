.seckillLmg{
	background: #fff;
	margin-top: 47px;
	padding: 4px 2px;
}
.seckillLmg .mui-col-xs-4{
	height: 120px;
	overflow: hidden;
}
.seckillLmg .mui-col-xs-4 img{
	display: block;
	width: 96%;
	height: 100%;
	margin: 0 auto;
}
.seckillLmg .mui-col-xs-4:nth-of-type(1){
