}
.cladssImg {
	background: #fff;
	border-bottom: 1px solid #F2F2F2;
}
.cladssImg .mui-col-xs-3{
	height: 120px;
}
.cladssImg .mui-col-xs-4 img{
	display: block;
	width: 100%;
	height: 100%;
}
.cladssImg .mui-col-xs-8 p:nth-of-type(1){
	color: #000000;
	margin-bottom: 0px;
	 display: -webkit-box;
    overflow: hidden;
    white-space: normal !important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.cladssImg .mui-col-xs-8 p:nth-of-type(2){
	color: #ff0000;
	margin-bottom: 0px;
	 display: -webkit-box;
    overflow: hidden;
    white-space: normal !important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.cladssImg .mui-col-xs-8 p:nth-of-type(3){
	color: #ff0000;
	margin-bottom: 0px;
	 display: -webkit-box;
    overflow: hidden;
    white-space: normal !important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    font-size: 18px;
}
.cladssImg .mui-col-xs-8 p:nth-of-type(3) del{
	color: #848689;
	margin-left: 4px;
	font-size: 14px;
}
.cladssImg .mui-col-xs-8 p:nth-of-type(3) b{
	float: right;
	font-weight: normal;
	font-size: 14px;
	margin-right: 10px;
	color: #ff0000;
	 display: -webkit-box;
    overflow: hidden;
    white-space: normal !important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.wohISThis{
	position: relative;
	margin: 2px 0px;
	padding:0px 3px;
	height: 20px;
}
.wohISThisLeft{
	float: left;
	width: 100%;
	position: relative;
}
.wohISThisRight{
	float: right;
	width: 39%;
	height: 35px;
}
.wohISThisRight button{
	border: none;
	width: 80px;
	/* background: #09b6f2; */
	color: #09b6f2;
	font-size: 14px;
	height: 30px;
	border-radius: 12px;
	text-align: center;
	padding: 2px 12px;
	border: 1px solid  #09b6f2;
}
.qiandaoL{
	position: absolute;
	height: 18px;
	width: 100%;
	border-radius: 20px;
	/* background: #e9a5a5; */
	border: 1px solid #09b6f2;
	top: 0px;
	left:0px
}
.qiandaoL .lanlan{
	position: absolute;
	height: 16px;
	width: 0%;
	border-radius: 20px;
	background: #09b6f2;
	top: 0px;
	color: #fff;
	text-align: center;
	font-size: 13px;
	line-height: 18px;
}
.rightLocalList{
	display: none;
}
.wohISThisRight button:active{
	color: #fff !important; 
	background: #09b6f2 !important;
}