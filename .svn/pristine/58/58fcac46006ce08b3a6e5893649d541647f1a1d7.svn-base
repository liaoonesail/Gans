.seckiliJshubox{
	background: #fff;
	margin: 4px 0px 0px 0px;
	border-bottom: 1px solid #F2F2F2;
}
.seckiliJshu{
	padding: 4px 10px;
	height: 35px;
	border-left: 2px solid #ff0000;
}
.seckiliJshu h5{
	color: #ff0000;
	font-size: 14px;
	float: left;
}
.seckiliJshu p{
	margin-bottom: 0px;
