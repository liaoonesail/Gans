<!DOCTYPE HTML>
<html lang="en-US">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge" >
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> 
	<link rel="stylesheet" href="css/frame.css">
	<link rel="stylesheet" href="css/minPublicBenefit.css">
	<link rel="stylesheet" href="css/common.css">
    <script type="text/javascript" src="js/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="js/popUpWin.js"></script>
    <script type="text/javascript" src="js/angular-1.0.1.min.js"></script>
    <script type="text/javascript" src="js/laydate.js"></script>
    <script type="text/javascript" src="js/miaosha.js"></script>
	<title>汇聚甘肃后台管理系统-商城管理-营销管理-秒杀管理</title>
</head>
<body>
	<div class="frameRall">
		<!-- 头部通知搜索栏 -->
		<div ng-app="">
            <div ng-include="'head.htm'"></div>
        </div>
		<div class="headlineBig ml40 mb20 clearfix">
			<h1 class="stair">商城管理<span>工作平台</span></h1>
			<h1 class="secondLevel">秒杀管理</h1>
		</div>
		<div class="prizeManage ml40 clearfix">
			<div class="commodityBox">
				<div class="commodityButBox clearfix mt30 mb30">
					<a href="javascript:;" class="butBox seckillAdd">添加秒杀活动</a>
					<!-- <input type="text" class="commodityInput ml30" id="miaoshaGoods" name="" value="" placeholder="商品名称" />
					<a href="javascript:;" class="butBox ml10">搜索</a> -->
				</div>
				<!-- 点击添加秒杀活动，弹出 -->
				<div class="seckillAddBox seckillWin">
					<i class="seckillClose"></i>
					<form id="fromAddMiaosha" action="" method="get">
						<ul>
							<input id="id" name="id" type="hidden" value=""/>
							<li>
								<select name="type2" style="border: solid 1px #D9D9D9;">
									<option value="">请选择二级分类</option>
								</select>
								<select name="type3" class="ml10" style="border: solid 1px #D9D9D9;">
									<option value="">请选择三级分类</option>
								</select>
							</li>
							<li>
								<span><i class="colorRed">*</i>秒杀商品：</span>
								<select name="goods_id" class="wi_300" style="border: solid 1px #D9D9D9;">
									<option value="0">请选择</option>
								</select>
							</li>
							<li>
								<span><i class="colorRed">*</i>活动标签：</span>
								<select name="label" class="wi_300" style="border: solid 1px #D9D9D9;">
									<!-- <option value="0">请选择</option>-->
									<option value="1">秒杀</option>
									<!-- <option value="2">团购</option>
									<option value="3">龙支付</option>-->
								</select>
							</li>
							<li>
								<span>开始时间：</span>
								<input type="text" class="seckillUpInp w120" placeholder="请选择开始时间" id="startDate" name="start_time" value="" />
								<span>结束时间：</span>
								<input type="text" class="seckillUpInp w120" placeholder="请选择结束时间" id="endDate" name="end_time" value="" />
							</li>
							<!-- <li>
								<span>结束时间：</span>
								<input type="text" class="seckillUpInp w120" placeholder="请选择结束时间" id="endDate" name="end_time" value="" />
							</li> -->
							<li>
								<span>秒杀价格：</span>
								<input type="text" class="seckillUpInp w120" placeholder="秒杀价格" id="real_price" name="real_price" value="" />
								<span>实际价格：</span>
								<input type="text" class="seckillUpInp w120" placeholder="实际价格" id="price" name="price" value="" disabled="disabled"/>
							</li>
							<li class="seckillUpBut"><a href="javascript:;" class="butBox addMiaoshaGoods">提交</a></li>
						</ul>
					</form>
				</div>
				<table class="commodityTab orderFormTab">
					<thead>
						<tr class="salesManageTr">
							<th>编号</th>
							<th>商品名称</th>
							<th>供应商名称</th>
							<th>开始时间</th>
							<th>结束时间</th>
							<th>秒杀价格</th>
							<th class="tc">操作</th>
						</tr>
					</thead>
					<tbody>
						<!-- <tr>
							<td>1</td>
							<td>秒杀活动1451451</td>
							<td>2017-06-02 08:00:00</td>
							<td>2017-06-02 23:00:00</td>
							<td>35</td>
							<td>已发布/已结束</td>
							<td>
								<span class="operationButBox">
									<a href="javascript:;" class="butBox ml10">编辑</a>
									<a href="javascript:;" class="butBox ml10">移除</a>
								</span>
							</td>
						</tr> -->
					</tbody>
					<tfoot class="commFootTd">
						<tr>
							<td colspan="5" class="commFootFont textar">总计<span id="miaosha_num">0</span>条数据&emsp;&emsp;分为<span id="pageCount">0</span>页&emsp;每页<span id="pageSize">10</span>个</td>
							<td colspan="3" class="commPageBut">
								<div class="commPageBut2 fr pr30">
									<a href="javascript:;" title="第一页" class="commPage1" onclick="firstpage()"></a>
									<a href="javascript:;" title="上一页" class="commPage2" onclick="prevpage()"></a>
									<select id="curpage" name="">
										<option value="">1</option>
									</select>
									<a href="javascript:;" title="下一页" class="commPage3" onclick="nextpage()"></a>
									<a href="javascript:;" title="尾页" class="commPage4" onclick="lastpage()"></a>
								</div>
							</td>
						</tr>
					</tfoot>
				</table>
			</div>
		</div>
	</div>
</body>
<script type="text/javascript">
	/**laydate.js**/
	var start = {
	     elem:"#startDate",
	     format: 'YYYY-MM-DD hh:mm:ss',
	     min: '2017-01-01 00:00:00', //最小日期
	     //max: laydate.now(),
	     istime: true, //是否开启时间选择
	        isclear: true, //是否显示清空
	        istoday: true, //是否显示今天
	        issure: true, //是否显示确认
	        festival: true //是否显示节日
	};
	var end = {
	     elem:"#endDate",
	     format: 'YYYY-MM-DD hh:mm:ss',
	     min: '2017-01-01 00:00:00', //最小日期
	     //max: laydate.now(),
	     istime: true, //是否开启时间选择
	     isclear: true, //是否显示清空
	     istoday: true, //是否显示今天
	     issure: true, //是否显示确认
	     festival: true //是否显示节日
	};
	laydate(start);
	laydate(end);
</script>
</html>