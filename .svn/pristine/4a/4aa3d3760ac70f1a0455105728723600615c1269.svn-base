<script>
    /*活动时间*/
    /**laydate.js**/
    var start = {
        elem : "#startDate",
        format : 'YYYY-MM-DD',
        /* min : laydate.now(), */
        max: laydate.now(),
        istime : false, //是否开启时间选择
        isclear : true, //是否显示清空
        istoday : true, //是否显示今天
        issure : true, //是否显示确认
        festival : true
    //是否显示节日
    };
    var end = {
        elem : "#endDate",
        format : 'YYYY-MM-DD',
        /* min : laydate.now(), */
