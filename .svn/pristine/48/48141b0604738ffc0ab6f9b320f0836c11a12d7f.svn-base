package com.lanzhou.entity;

public class Baobiao {
	private String area;//地区
	private int countNum;//总捐款次数
	private double totlePrice;//总捐款金额
	private int countPerson;//总捐款人数
	private int ZcountNum;//昨日捐款次数
	private double ZtotlePrice;//昨日捐款金额
	private int ZcountPerson;//昨日捐款人数
	public String getArea() {
		return area;
	}
	public void setArea(String area) {
		this.area = area;
	}
	public int getCountNum() {
		return countNum;
	}
	public void setCountNum(int countNum) {
		this.countNum = countNum;
	}
	public double getTotlePrice() {
		return totlePrice;
	}
	public void setTotlePrice(double totlePrice) {
		this.totlePrice = totlePrice;
	}
	public int getCountPerson() {
		return countPerson;
	}
	public void setCountPerson(int countPerson) {
		this.countPerson = countPerson;
	}
	public int getZcountNum() {
		return ZcountNum;
	}
	public void setZcountNum(int zcountNum) {
		ZcountNum = zcountNum;
	}
	public double getZtotlePrice() {
		return ZtotlePrice;
	}
	public void setZtotlePrice(double ztotlePrice) {
		ZtotlePrice = ztotlePrice;
	}
	public int getZcountPerson() {
		return ZcountPerson;
	}
	public void setZcountPerson(int zcountPerson) {
		ZcountPerson = zcountPerson;
	}
	public Baobiao(String area, int countNum, double totlePrice,
			int countPerson, int zcountNum, double ztotlePrice, int zcountPerson) {
		super();
		this.area = area;
		this.countNum = countNum;
		this.totlePrice = totlePrice;
		this.countPerson = countPerson;
		ZcountNum = zcountNum;
		ZtotlePrice = ztotlePrice;
		ZcountPerson = zcountPerson;
	}
	public Baobiao() {
		super();
	}
	

}
