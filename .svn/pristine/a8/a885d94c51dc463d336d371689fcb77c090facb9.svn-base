package com.lanzhou.entity;

public class Prize {
	private int id;//
	private int level;//奖项等级：  1:一等奖;2:二等奖;3:三等奖;4:四等奖;5:五等奖;6:六等奖;
	private String name;//奖品名
	private int probability;//中奖概率 千分之几
	private int amount;//开奖数量
	private int overplus;//剩余数量
	private String type;//奖品类别 1实物 2虚拟
	private String indate;//有效期
	private int status;//是否上线 0否 1是
	private String orderNum;//捐款订单号
	
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getLevel() {
		return level;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getProbability() {
		return probability;
	}

	public void setProbability(int probability) {
		this.probability = probability;
	}

	public int getAmount() {
		return amount;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}

	public int getOverplus() {
		return overplus;
	}

	public void setOverplus(int overplus) {
		this.overplus = overplus;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getIndate() {
		return indate;
	}

	public void setIndate(String indate) {
		this.indate = indate;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}
	
	public String getOrderNum() {
		return orderNum;
	}

	public void setOrderNum(String orderNum) {
		this.orderNum = orderNum;
	}
	
	public Prize(int id, int level, String name, int probability, int amount,
			int overplus, String type, String indate, int status,
			String orderNum) {
		super();
		this.id = id;
		this.level = level;
		this.name = name;
		this.probability = probability;
		this.amount = amount;
		this.overplus = overplus;
		this.type = type;
		this.indate = indate;
		this.status = status;
		this.orderNum = orderNum;
	}

	public Prize(int id, int level, String name, int probability, int amount,
			int overplus, String type, String indate, int status) {
		super();
		this.id = id;
		this.level = level;
		this.name = name;
		this.probability = probability;
		this.amount = amount;
		this.overplus = overplus;
		this.type = type;
		this.indate = indate;
		this.status = status;
	}

	public Prize() {
		super();
	}
	
	@Override
	public String toString() {
		return "Prize [id=" + id + ", level=" + level + ", name=" + name
				+ ", probability=" + probability + ", amount=" + amount
				+ ", overplus=" + overplus + ", type=" + type + ", indate="
				+ indate + ", status=" + status + "]";
	}
}
