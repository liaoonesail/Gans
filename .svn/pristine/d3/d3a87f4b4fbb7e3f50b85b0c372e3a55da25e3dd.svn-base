package com.lanzhou.entity;

public class Area {
	private int id;
	private String name;//地区名
	private int sort;//排序
	private int sr_amount;//善融支付点击次数
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSort() {
		return sort;
	}
	public void setSort(int sort) {
		this.sort = sort;
	}
	public int getSr_amount() {
		return sr_amount;
	}
	public void setSr_amount(int sr_amount) {
		this.sr_amount = sr_amount;
	}

	public Area(int id, String name, int sort, int sr_amount) {
		this.id = id;
		this.name = name;
		this.sort = sort;
		this.sr_amount = sr_amount;
	}

	public Area(String name, int sort, int sr_amount) {
		this.name = name;
		this.sort = sort;
		this.sr_amount = sr_amount;
	}

	public Area() {
		super();
	}
	

}
